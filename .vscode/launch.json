{
    "configurations": [
        {
            "type": "code-d",
            "request": "launch",
            // "dubBuild": false,
            "preLaunchTask": "dub build",
            "name": "Build & Debug DUB project",
            "cwd": "${command:dubWorkingDirectory}",
            "program": "build/${command:dubTarget}",
            "debugger": "lldb",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": true
            }
        }
    ]
}